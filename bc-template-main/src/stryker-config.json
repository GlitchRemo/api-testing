{
    "stryker-config": {
        "solution": "./NGrid.Customer.Template.sln",
        "ignore-methods": [
            "_logger.*",
            "Logger.*"
        ],
        "mutation-level": "Complete",
        "reporters": [
            "html",
            "json",
            "progress",
            "markdown"
        ],
        "since": {
            "enabled": true,
            "target": "main",
            "ignore-changes-in": [
                "src/StrykerOutput/**",
                "src/stryker-config.json"
            ]
        },
        "baseline": {
            "enabled": true,
            "provider": "Disk"
        }
    }
}